{
    "schmema": "https://schemas.runtipi.io/dynamic-compose.json",
    "services": [
      {
        "name": "dumbassets",
        "image": "dumbwareio/dumbassets:1.0.0",
        "isMain": true,
        "internalPort": 3000,
        "environment": {
          "NODE_ENV": "production",
          "BASE_URL": "${APP_PROTOCOL:-http}://{APP_DOMAIN}",
          "TZ": "${TZ}"
        },
        "volumes": [
          {
            "hostPath": "${APP_DATA_DIR}/data/data",
            "containerPath": "/app/data"
          }
        ]
      }
    ]
  }